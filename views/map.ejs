    <div onclick="someFunction(this);" class="12" style="border:3px solid black;width:25px; float:left;">12</div>
    <div onclick="someFunction(this);" class="13" style="border:3px solid black;width:25px; float:left;">13</div>
    <div onclick="someFunction(this);" class="14" style="border:3px solid black;width:25px; float:left;">14</div>
</div>

<div class="row" id="r2">
    <div onclick="someFunction(this);" class="21" style="border:3px solid black;width:25px; float:left;">21</div>
    <div onclick="someFunction(this);" class="22" style="border:3px solid black;width:25px; float:left;">22</div>
    <div onclick="someFunction(this);" class="23" style="border:3px solid black;width:25px; float:left;">23</div>
    <div onclick="someFunction(this);" class="24" style="border:3px solid black;width:25px; float:left;">24</div>
</div>
<div class="row" id="r3">
